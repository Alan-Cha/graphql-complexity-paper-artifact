{
  "name": "graphql-query-generator",
  "version": "0.4.0",
  "description": "Randomly generate a GraphQL query using a GraphQL schema",
  "author": "Erik Wittern",
  "contributors": [
    "Alan Cha"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "husky": "^4.2.5",
    "jest": "^26.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "graphql": "^14.0.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "seedrandom": "^3.0.5"
  }
}
